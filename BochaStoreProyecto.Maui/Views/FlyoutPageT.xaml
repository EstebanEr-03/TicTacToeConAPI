<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BochaStoreProyecto.Maui.Views.FlyoutPageT"
             xmlns:local="clr-namespace:BochaStoreProyecto.Maui.Views"            
             xmlns:local1="clr-namespace:BochaStoreProyecto.Maui"            
             Title="FlyoutPageT">
    <FlyoutPage.Flyout>
        <local:FlyoutMenuPage x:Name="flyoutPage"/>
    </FlyoutPage.Flyout>
    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <local1:MainPage/>
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>
</FlyoutPage>